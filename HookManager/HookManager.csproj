<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFrameworks>net35;net40;net45;net461;net48;netcoreapp3.1; net5.0-windows; net6.0-windows</TargetFrameworks>
	  <UseWindowsForms>true</UseWindowsForms>
	  <IsPublishable>False</IsPublishable>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <PackageId>HookManager</PackageId>
	  <PackageTags>Win32;API;Hook;Mouse;Keyboard;HotKey</PackageTags>
	  <Authors>raoyutian</Authors>
	  <Company />
	  <Description>A library of global keyboard, mouse and hotkey hooks that can be used under windows</Description>
	  <PackageProjectUrl> </PackageProjectUrl>
	  <RepositoryUrl>https://github.com/raoyutian/HookManager</RepositoryUrl>
	  <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
	  <AssemblyVersion>1.0.0</AssemblyVersion>
	  <FileVersion>1.0.0</FileVersion>
	  <Version>1.0.0</Version>
  </PropertyGroup>
</Project>
